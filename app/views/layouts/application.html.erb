<!DOCTYPE html>
<html>
  <head>
    <title>Blog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <%= current_user.name if current_user%>
  <p>
  <%= link_to 'Выйти', session_logout_path %>
    <%= link_to 'Войти', session_login_path %>
    <%= link_to 'Регистрация', registration_path %>
    <%= yield %>
  </body>
</html>
